<!DOCTYPE html>
<html lang="pl">

<head>
  <meta charset="UTF-8">
  <meta http-equiv="X-UA-Compatible" content="IE=edge">
  <meta name="viewport" content="width=device-width, initial-scale=1.0">


  <meta name="author" content="Forest Company">
  <meta name="keywords" content="las, odpoczynek, świeży zapach,  relaks">

  <meta name="description"
    content="Skontaktuj się ze specjalistami Forest aby dowiedzieć się o szczegółach pobytu w wyjątkowym lesie pełnym ciszy i idealnych przygód">

  <title>Forest Website</title>

  <!-- google fonts -->
  <link rel="preconnect" href="https://fonts.googleapis.com">
  <link rel="preconnect" href="https://fonts.gstatic.com" crossorigin>
  <link href="https://fonts.googleapis.com/css2?family=Raleway:wght@400;700&display=swap" rel="stylesheet">

  <!-- fontaweseome -->
  <script src="https://kit.fontawesome.com/81dc70932b.js" crossorigin="anonymous"></script>

  <link rel="stylesheet" href="dist/css/style.min.css">
</head>

<body>
  <!-- @include 'navcontact.kit' -->

  <!-- @include 'header.kit' -->

  <section class="info">
    <h1>Kontakt do biura lasu:</h1>
    <p class="street">ul: Mcdonalda 123 Kraków 12-345</p>
    <p class="phone-number">Telefon: 555 555 555</p>
  </section>

  <section class="contact-form">
    <div class="contact-wrapper">
      <form>

        <h2>Skontaktuj się z nami:</h2>
        <div class="form-box">
          <label for="username">Twoje imię:</label>
          <input type="text" id="username" placeholder="Podaj nazwę użytkownika" required>
        </div>

        <div class="form-box">
          <label for="email">Adres e-mail:</label>
          <input type="email" id="email" placeholder="Podaj adres mailowy" required>
        </div>

        <div class="form-box">
          <label for="message">Treść wiadomości:</label>
          <textarea placeholder="Wpisz tu swoją wiadomość do nas" for="message" class="message"></textarea>
        </div>


        <div class="control-buttons">
          <button class="send" type="submit" onclick="void(0)">Wyślij</button>
          <button class="clear" type="reset" onclick="void(0)">Wyczyść</button>
        </div>

      </form>
    </div>
  </section>

  <section class="google-maps">
    <h2 class="find-us">Przyjedź do nas:</h2>
    <iframe
      src="https://www.google.com/maps/embed?pb=!1m18!1m12!1m3!1d2559.8982686590143!2d19.890660215892424!3d50.0881915212814!2m3!1f0!2f0!3f0!3m2!1i1024!2i768!4f13.1!3m3!1m2!1s0x47165a326d015c3b%3A0x47a12cd367a1f34e!2sRestauracja%20McDonald&#39;s!5e0!3m2!1spl!2spl!4v1672408711861!5m2!1spl!2spl"
      width="600" height="450" style="border:0;" allowfullscreen="" loading="lazy"
      referrerpolicy="no-referrer-when-downgrade" class="map"></iframe>
  </section>

  <!-- @include 'footer.kit' -->

  <script src="dist/js/main.min.js"></script>
</body>

</html>